<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="108dp"
    android:height="108dp"
    android:viewportWidth="108"
    android:viewportHeight="108">
    
    <!-- Winamp lightning bolt style icon -->
    <group android:translateX="54" android:translateY="54">
        
        <!-- Main lightning bolt -->
        <path android:pathData="M-8,-20 L8,-20 L-2,-5 L6,-5 L-8,20 L-18,5 L-10,5 Z"
            android:fillColor="#FF00FF00"
            android:strokeColor="#FF00CC00"
            android:strokeWidth="1"/>
        
        <!-- Glow effect -->
        <path android:pathData="M-8,-20 L8,-20 L-2,-5 L6,-5 L-8,20 L-18,5 L-10,5 Z"
            android:fillColor="#3300FF00"
            android:scaleX="1.2"
            android:scaleY="1.2"/>
        
        <!-- Digital display style text "MP" -->
        <group android:translateY="25">
            <!-- M -->
            <path android:pathData="M-15,-3 L-15,3 M-15,-3 L-12,0 M-12,0 L-9,-3 M-9,-3 L-9,3"
                android:strokeColor="#FF00FF00"
                android:strokeWidth="1.5"
                android:strokeLineCap="round"/>
            
            <!-- P -->
            <path android:pathData="M-6,-3 L-6,3 M-6,-3 L-3,-3 L-3,0 L-6,0"
                android:strokeColor="#FF00FF00"
                android:strokeWidth="1.5"
                android:strokeLineCap="round"/>
        </group>
        
        <!-- Small equalizer bars -->
        <group android:translateX="12" android:translateY="8">
            <path android:pathData="M0,-4 L2,-4 L2,4 L0,4 Z" android:fillColor="#FF00FF00"/>
            <path android:pathData="M4,-6 L6,-6 L6,6 L4,6 Z" android:fillColor="#FF00FF00"/>
            <path android:pathData="M8,-3 L10,-3 L10,3 L8,3 Z" android:fillColor="#FF00FF00"/>
            <path android:pathData="M12,-5 L14,-5 L14,5 L12,5 Z" android:fillColor="#FF00FF00"/>
        </group>
        
    </group>
</vector>